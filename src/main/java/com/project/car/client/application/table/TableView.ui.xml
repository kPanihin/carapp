<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:select="urn:import:org.gwtbootstrap3.extras.select.client.ui">

    <g:HTMLPanel>
        <g:SimplePanel ui:field="panel"/>

        <b:Column size="MD_2" styleName="col-md-2 sidebar">
            <b:NavPills ui:field="floorsNav" styleName="nav nav-sidebar" />
        </b:Column>

        <b:Column size="MD_8">
            <b:Breadcrumbs ui:field="sectionsBread"/>
        </b:Column>

        <b:Button ui:field="back" text="Back" marginLeft="20"/>
    </g:HTMLPanel>
</ui:UiBinder>